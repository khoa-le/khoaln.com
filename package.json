    "start": "gatsby serve --port 5020",
    "setup": "npm install && npm run validate",
    "clean": "gatsby clean",
    "validate": "npm run build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|graphql|md|mdx)\"",
